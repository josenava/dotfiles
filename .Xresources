URxvt.intensityStyles:	false
URxvt*background:       [85]#000000
URxvt*foreground:       #F1EFEE
URxvt*depth:		32
URxvt.font: xft:RobotoMono Nerd Font Mono:style=Medium,Regular:size=18
URxvt.boldFont: xft: RobotoMono Nerd Font Mono:style=Medium,Bold:size=18
URxvt.italicFont: xft: RobotoMono Nerd Font Mono:style=Medium,Italic:size=18
URxvt.boldItalicFont: xft: RobotoMono Nerd Font Mono:style=Bold Italic:size=18
URxvt.scrollBar:	false
URxvt.cursorColor: 	white
URxvt.perl-ext-common:      default,tabbedex,clipboard,url-select,keyboard-select,font-size
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.M-Up:     perl:font-size:increase
URxvt.keysym.C-m:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
URxvt.keysym.C-equal:  perl:font-size:reset
URxvt.keysym.C-slash:  perl:font-size:show


rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #FFA500, #ffffff
rofi.color-active:	#FFA500, #FFA500, #FFA500, #FFA500, #FFB266
rofi.color-urgent:	#FFA500, #FFA500, #FFA500, #FFA500, #FFB266

rofi.fake-transparency:	true
rofi.lines:		3
rofi.bw:		0
rofi.opacity:		"10"
rofi.hide-scrollbar:	true
rofi.width: 30
