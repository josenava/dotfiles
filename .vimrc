set rtp+=/usr/lib/python3.7/site-packages/powerline/bindings/vim

filetype on
syntax on

colorscheme monokai
set t_Co=256

set tabstop=4
set softtabstop=4
set number
set showmatch
set hlsearch
set incsearch
set cursorline

set laststatus=2

let g:powerline_pycmd = "py3"
